{
    "name": "kirby-vite-basic-kit",
    "main": "src/index.js",
    "type": "module",
    "scripts": {
        "vite": "vite",
        "predev": "npm run i18n:build",
        "dev": "concurrently \"npm:server\" \"npm:vite\"",
        "server": "php -S localhost:8888 -t public server.php",
        "prebuild": "npm run i18n:build:ci",
        "build": "vite build",
        "preview": "npm run build && npm run server",
        "prettier": "prettier",
        "i18n:build": "ts-node-esm ./src/scripts/build-lang-files.ts && prettier --parser json-stringify --write src/locales/*.json",
        "i18n:build:ci": "ts-node-esm ./src/scripts/build-lang-files.ts"
    },
    "author": "arnoson",
    "license": "MIT",
    "devDependencies": {
        "@intlify/vite-plugin-vue-i18n": "^6.0.3",
        "@prettier/plugin-php": "^0.19.1",
        "@tailwindcss/typography": "^0.5.7",
        "@types/node": "^18.7.21",
        "@typescript-eslint/eslint-plugin": "^5.38.0",
        "@typescript-eslint/parser": "^5.38.0",
        "@vitejs/plugin-vue": "^3.1.0",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^11.0.2",
        "autoprefixer": "^10.4.12",
        "concurrently": "^7.1.0",
        "cssnano": "^5.1.13",
        "eslint": "^8.24.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.8",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-vue": "^9.5.1",
        "path": "^0.12.7",
        "postcss": "^8.4.16",
        "postcss-rem": "^2.0.2",
        "prettier": "^2.7.1",
        "public-google-sheets-parser": "^1.2.6",
        "tailwindcss": "^3.1.8",
        "ts-node": "^10.9.1",
        "vite": "^3.0.0",
        "vite-plugin-kirby": "^0.1.0",
        "vite-plugin-svg-icons": "^2.0.1"
    },
    "dependencies": {
        "vue": "^3.2.40",
        "vue-i18n": "^9.2.2"
    }
}
